\ \_/ / | | | | | | | | | |  | | |_| | |/ /|_|     
 \___/|_| |_| |_|_| |_|_\_|  |_/\___/|___/ (_)     
                    </pre>                    
                </div>
            </div>

            <div id="input">
                <input type="text" name="commandInput" id="commandInput" />
            </div>
        </div>
    </main>

    <script>
        document.getElementById('commandInput').focus();
    </script>
</body>
</html>